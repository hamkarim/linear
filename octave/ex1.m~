misc_funcs;  % load some useful functions

yes = "y";
no = "n";
 
function prob1 ()
m = rand_int(5);
n = rand_int(5);
A = rand (m,n)
B = rand (m,n)
yn = input ("Are they equal? (1: yes 0: no) ");

if yn == yes
  yn = 0;
elseif yn == n
  yn =1;
endif;
if yn == (A ==B)
  printf ("Correct \n");
 else 
   printf("Incorrect \n");
endif;
endfunction;


probs = ["prob1";];

probs = cellstr(probs);

p1 = menu ("Exercises 1: Matrix Operations", "Matrix Equality");
feval(probs{p1});


